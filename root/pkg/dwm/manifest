[env]

[namespace]
/app:rx = $PKG
/net:rw = /net
/dev/fb:rw = /dev/fb
/dev/kbd:rw = /dev/kbd
/dev/mouse:rw = /dev/mouse
/dev/klog:rw = /dev/klog
/dev/const:rw = /dev/const
/dev/shmem:rw = /dev/shmem